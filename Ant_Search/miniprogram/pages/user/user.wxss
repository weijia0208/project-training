  color:#333;
  padding-left:10px;
  
}
.my-goods{
  height:60px;
  border-bottom:1px #eee solid;
}
.my-image{
  position:relative;
  width:25px;
  height:25px;
  margin-top:20px;
  float:left;
}
.my-text{
  position: absolute;
  display:block;
  margin-left:30px;
  margin-top:22px;
}
